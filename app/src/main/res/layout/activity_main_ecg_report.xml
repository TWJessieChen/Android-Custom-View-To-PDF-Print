<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/one_twelve_group"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:visibility="visible">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal">

            <FrameLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:background="@drawable/ecg_report_ecg_data_rectangle_write"
                android:layout_marginStart="2dp"
                android:layout_marginTop="2dp">

                <com.jc666.customviewtopdfprint.StaticEcgBackgroundView
                    android:id="@+id/background_one_twelve_wave_view_i"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_margin="1dp"
                    app:wave_line_second="10"
                    app:grid_label_name="I"
                    app:draw_mode="loop"
                    app:grid_visible="true"
                    app:wave_background="#ff202123"
                    app:wave_line_color="#ff00c100"/>

                <HorizontalScrollView
                    android:id="@+id/hs_two_six_lead_v1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="50dp">

                    <com.jc666.customviewtopdfprint.StaticWaveEcgView
                        android:id="@+id/one_twelve_wave_view_i"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_margin="1dp"
                        app:draw_mode="loop"
                        app:grid_label_name="I"
                        app:grid_visible="true"
                        app:wave_background="#ff202123"
                        app:wave_line_color="#ff00c100"
                        app:wave_line_second="10"
                        />

                </HorizontalScrollView>


            </FrameLayout>

            <FrameLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:background="@drawable/ecg_report_ecg_data_rectangle_write"
                android:layout_marginStart="5dp"
                android:layout_marginTop="2dp"
                android:layout_marginEnd="2dp">

                <com.jc666.customviewtopdfprint.StaticWaveEcgView
                    android:id="@+id/one_twelve_wave_view_ii"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_margin="1dp"
                    app:wave_line_second="10"
                    app:grid_label_name="I I"
                    app:draw_mode="loop"
                    app:wave_background="#ff202123"
                    app:wave_line_color="#ff00c100"
                    app:grid_visible="true"/>

            </FrameLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal">

            <FrameLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:background="@drawable/ecg_report_ecg_data_rectangle_write"
                android:layout_marginStart="5dp"
                android:layout_marginTop="5dp">

                <com.jc666.customviewtopdfprint.StaticWaveEcgView
                    android:id="@+id/one_twelve_wave_view_iii"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_margin="1dp"
                    app:wave_line_second="10"
                    app:grid_label_name="I I I"
                    app:draw_mode="loop"
                    app:wave_background="#ff202123"
                    app:wave_line_color="#ff00c100"
