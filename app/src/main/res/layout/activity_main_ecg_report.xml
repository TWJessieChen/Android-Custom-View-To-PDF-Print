                <com.jc666.customviewtopdfprint.StaticWaveEcgView
                    android:id="@+id/one_twelve_wave_view_avr"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_margin="1dp"
                    app:wave_line_second="10"
                    app:grid_label_name="a V R"
                    app:draw_mode="loop"
                    app:wave_background="#ff202123"
                    app:wave_line_color="#ff00c100"
                    app:grid_visible="true"
                    tools:ignore="MissingConstraints"/>

            </FrameLayout>

            <FrameLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:background="@drawable/ecg_report_ecg_data_rectangle_write"
