                    app:grid_visible="true"
                    tools:ignore="MissingConstraints"/>

            </FrameLayout>

            <FrameLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:background="@drawable/ecg_report_ecg_data_rectangle_write"
