                    android:layout_margin="1dp"
                    app:wave_line_second="10"
                    app:grid_label_name="a V F"
                    app:draw_mode="loop"
                    app:wave_background="#ff202123"
                    app:wave_line_color="#ff00c100"
                    app:grid_visible="true"
                    tools:ignore="MissingConstraints"/>

            </FrameLayout>

        </LinearLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="1dp"
